# DATABASE_URL=postgres://postgres:postgres@localhost:5432/ticketing
# REDIS_URL=redis://localhost:6379
# JWT_SECRET=your_secret_here
# For CORS: allow frontend origin(s). Comma-separated for multiple. Local UI runs on 3002.
# FRONTEND_URL=http://localhost:3002
# STRIPE_SECRET_KEY=your_stripe_secret_key_here
# STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Security (optional; defaults shown)
# RATE_LIMIT_MAX=200
# RATE_LIMIT_WINDOW_MS=900000
# AUTH_RATE_LIMIT_MAX=10
# AUTH_RATE_LIMIT_WINDOW_MS=900000
# BODY_LIMIT=1048576

# Logging: debug | info | warn | error (default: info)
# LOG_LEVEL=info

# OpenTelemetry tracing (optional)
# OTEL_SERVICE_NAME=ticketing-api
# OTEL_SDK_DISABLED=true
# OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
# OTEL_TRACES_EXPORTER=otlp|console|none
